    var mem = $("#mem").get(0).getContext("2d");
    var mem_data = {
            labels : [{% for stat in system.stats %}"{{ stat.timestamp|date("H:i") }}h"{% if not loop.last %},{% endif %}{% endfor %}],
            datasets : [
                    {
                            fillColor : "rgba(220,220,220,0.5)",
                            strokeColor : "rgba(220,220,220,1)",
                            pointColor : "rgba(220,220,220,1)",
                            pointStrokeColor : "#fff",
                            data : [{% for stat in system.stats %}{{ stat.mem }}{% if not loop.last %},{% endif %}{% endfor %}]
                    }
            ]
    }
    new Chart(mem).Line(mem_data,options);